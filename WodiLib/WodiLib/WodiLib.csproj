<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <Nullable>enable</Nullable>
        <PackageId>WodiLib</PackageId>
        <PackageVersion>2.3.1</PackageVersion>
        <Copyright>Copyright 2019 kameske</Copyright>
        <Version>2.3.1</Version>
        <PackageProjectUrl>https://github.com/kameske/WodiLib</PackageProjectUrl>
        <PackageLicenseUrl />
        <RepositoryUrl>https://github.com/kameske/WodiLib.git</RepositoryUrl>
        <RepositoryType>Git</RepositoryType>
        <Title>WodiLib</Title>
        <Authors>Kameske</Authors>
        <RootNamespace>WodiLib</RootNamespace>
        <Product>WodiLib</Product>
        <AssemblyVersion>2.3.1</AssemblyVersion>
        <AssemblyName>WodiLib</AssemblyName>
        <LangVersion>8</LangVersion>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
      <DocumentationFile>bin\Debug\WodiLib.xml</DocumentationFile>
    </PropertyGroup>
    <PropertyGroup Condition="'$(Configuration)'=='Release'">
      <DocumentationFile>bin\Release\WodiLib.xml</DocumentationFile>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="System.Text.Encoding.CodePages" Version="4.7.0" />
    </ItemGroup>

</Project>
