<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>WodiLib</RootNamespace>
    <TargetFrameworks>net461;netstandard2.0;netcoreapp2.1</TargetFrameworks>
    <PackageVersion>1.2.0.1</PackageVersion>
    <Title>WodiLib</Title>
    <Authors>kameske</Authors>
    <Copyright>MIT License Copyright(c) 2019 kameske</Copyright>
    <Version>1.2.1</Version>
    <PackageProjectUrl>https://github.com/kameske/WodiLib</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/kameske/WodiLib/blob/master/LICENSE</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/kameske/WodiLib.git</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <AssemblyVersion>1.2.1.0</AssemblyVersion>
    <FileVersion>1.2.1.0</FileVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\netstandard2.0\WodiLib.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Text.Encoding.CodePages" Version="4.5.1" />
  </ItemGroup>
  <ItemGroup>
    <Reference Include="I18N">
      <HintPath>..\..\..\..\..\Unity\Projects\PokeBattle\Assets\Plugins\I18N.dll</HintPath>
    </Reference>
    <Reference Include="I18N.CJK">
      <HintPath>..\..\..\..\..\Unity\Projects\PokeBattle\Assets\Plugins\I18N.CJK.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>