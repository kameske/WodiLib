<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <RootNamespace>WodiLib</RootNamespace>
    <TargetFrameworks>net461;netstandard2.0;netcoreapp2.1</TargetFrameworks>
    <PackageVersion>1.3.1</PackageVersion>
    <Title>WodiLib</Title>
    <Authors>kameske</Authors>
    <Copyright>Copyright 2019 kameske</Copyright>
    <Version>1.3.1</Version>
    <PackageProjectUrl>https://github.com/kameske/WodiLib</PackageProjectUrl>
    <PackageLicenseUrl />
    <RepositoryUrl>https://github.com/kameske/WodiLib.git</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <AssemblyVersion>1.3.1</AssemblyVersion>
    <FileVersion>1.3.1</FileVersion>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\netstandard2.0\WodiLib.xml</DocumentationFile>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
    <DocumentationFile>bin\Release\netstandard2.0\WodiLib.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.Text.Encoding.CodePages" Version="4.5.1" />
  </ItemGroup>
</Project>
